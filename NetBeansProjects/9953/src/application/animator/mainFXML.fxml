<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="anchorPane" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.animator.mainFXMLController">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" spacing="20.0">
         <children>
            <Button fx:id="processSchedulerButton" mnemonicParsing="false" onAction="#populateSchedulerAnimator" text="Scheduler Animator" />
            <Button mnemonicParsing="false" onAction="#launchPageReplacementAnimator" text="Page Replacement Animator" />
            <Button mnemonicParsing="false" onAction="#launchBankersAlgorithmAnimator" text="Bankers Algorithm Animator" />
         </children>
      </VBox>
   </children>
</AnchorPane>
